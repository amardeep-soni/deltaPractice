<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Wonderlust</title>
  </head>
  <body>
    <h1>All Listings</h1>
    <form action="/listings/new">
      <button>Add new listing</button>
    </form>
    <ul>
      <% for (const listing of listings) { %>
      <li>
        <a href="/listings/<%= listing._id %>">
          <%= listing.title %>
        </a>
      </li>
      <% } %>
    </ul>
  </body>
</html>
