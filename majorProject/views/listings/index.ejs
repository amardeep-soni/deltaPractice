<% layout("/layouts/boilerplate.ejs") %>

<body>
  <h2>All Listings</h2>
  <form action="/listings/new">
    <button>Add new listing</button>
  </form>
  <ul>
    <% for (const listing of listings) { %>
    <li>
      <a href="/listings/<%= listing._id %>"> <%= listing.title %> </a>
    </li>
    <% } %>
  </ul>
</body>
